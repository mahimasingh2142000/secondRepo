{
    "name": "vendor/my-package",
    "description": "My first package ",
    "keywords": [
        "vendor",
        "my-package"
    ],
    "homepage": "https://github.com/vendor/my-package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mahima Singh",
            "email": "amit.singhanu05@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\MyPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\MyPackage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
